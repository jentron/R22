<?xml version="1.0"?>

<PropertyList>

<path>rpm.ac</path>

    <animation>
        <type>rotate</type>
        <object-name>RPM.Eneedle</object-name>
        <property>engines/engine/n1</property>
        <interpolation>
            <entry><ind>0.0</ind><dep>0</dep></entry>
            <entry><ind>110</ind><dep>85.0</dep></entry>
        </interpolation>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>-0.027</y-m>
            <z-m>0.0</z-m>
        </center>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>RPM.Rneedle</object-name>
        <property>rotors/main/rpm</property>
        <interpolation>
            <entry><ind>0.0</ind><dep>0</dep></entry>
            <entry><ind>243.0</ind><dep>4.5</dep></entry>
            <entry><ind>599.0</ind><dep>91.0</dep></entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0.027</y-m>
            <z-m>0.0</z-m>
        </center>
    </animation>

</PropertyList>